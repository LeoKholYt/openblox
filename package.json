{
  "name": "openblox",
  "version": "0.0.3",
  "files": [
    "/dist"
  ],
  "exports_PROD": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./apis": {
      "types": "./dist/apis/index.d.ts",
      "default": "./dist/apis/index.js"
    },
    "./apis/cacheAdapters/redis": {
      "types": "./dist/apiCacheAdapters/redis/index.d.ts",
      "default": "./dist/apiCacheAdapters/redis/index.js"
    },
    "./interfaces": {
      "types": "./dist/interfaces/index.d.ts",
      "default": "./dist/interfaces/index.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client/index.ts",
    "./apis": "./src/apis/index.ts",
    "./apis/cacheAdapters/redis": "./src/apiCacheAdapters/redis.ts",
    "./interfaces": "./src/interfaces/index.ts",
    "./errors": "./src/errors.ts"
  },
  "scripts": {
    "build": "del-cli --force dist && tsup src --format cjs,esm --dts"
  },
  "devDependencies": {
    "@swc/core": "^1.3.76",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.4.9",
    "del-cli": "^5.0.0",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "ioredis": "^5.3.2",
    "lodash.merge": "^4.6.2",
    "p-iteration": "^1.1.8"
  }
}
